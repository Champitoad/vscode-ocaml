{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "debug-debugger",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}",
			"program": "${workspaceRoot}/../../out/src/debug/debug.js",
			"stopOnEntry": false,
			"args": [ "--server=4711" ],
			"sourceMaps": true,
            "env": {
                "DEBUG_VSCODE_OCAML": "development"
            },
			"outFiles": [
				"${workspaceRoot}/../../out/src/debug"
			]
		}
	]
}